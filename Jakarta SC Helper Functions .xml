<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update sys_domain="global" table="catalog_script_client"&gt;&lt;catalog_script_client action="INSERT_OR_UPDATE"&gt;&lt;active&gt;true&lt;/active&gt;&lt;applies_catalog&gt;false&lt;/applies_catalog&gt;&lt;applies_extended&gt;false&lt;/applies_extended&gt;&lt;applies_req_item&gt;true&lt;/applies_req_item&gt;&lt;applies_sc_task&gt;true&lt;/applies_sc_task&gt;&lt;applies_target_record&gt;false&lt;/applies_target_record&gt;&lt;applies_to&gt;set&lt;/applies_to&gt;&lt;cat_item/&gt;&lt;cat_variable/&gt;&lt;condition/&gt;&lt;description/&gt;&lt;field/&gt;&lt;global&gt;true&lt;/global&gt;&lt;messages/&gt;&lt;name&gt;onLoad_Backend_Functions&lt;/name&gt;&lt;order/&gt;&lt;script&gt;&lt;![CDATA[function onLoad() {
	//Type appropriate comment here, and begin script below
}

function forceField(){

	this.full = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	

			var fieldz = g_form.getValue(hiddenfields[j]);
			if (fieldz == '' || fieldz == ' ' || fieldz == 'false'){
				g_form.setMandatory(hiddenfields[j], false);  
				g_form.setDisplay(hiddenfields[j], false);
			}
		}
	};


	this.show = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setDisplay(hiddenfields[j], true);
		}
	};

	this.showMan = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setDisplay(hiddenfields[j], true);
			g_form.setMandatory(hiddenfields[j], true);
		}
	};


	this.hide = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setMandatory(hiddenfields[j], false);
			g_form.setDisplay(hiddenfields[j], false);
		}
	};


	this.clear = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.clearValue(hiddenfields[j]);
		}
	};


	this.nul = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setValue(hiddenfields[j],'');
		}
	};


	this.noMan = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setMandatory(hiddenfields[j], false);
		}
	};

	this.read = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setReadOnly(hiddenfields[j], true);
		}
	};

	this.noRead = function(list){
		var hiddenfields = list, j;
		for (j = 0; j &lt; hiddenfields.length; ++j) {	
			g_form.setReadOnly(hiddenfields[j], false);
		}
	};

}

]]&gt;&lt;/script&gt;&lt;sys_class_name&gt;catalog_script_client&lt;/sys_class_name&gt;&lt;sys_created_by&gt;JLINC08&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2017-08-21 02:45:38&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;6a733b40dbfc47c03217f3861d9619b0&lt;/sys_id&gt;&lt;sys_mod_count&gt;18&lt;/sys_mod_count&gt;&lt;sys_name&gt;onLoad_Backend_Functions&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;catalog_script_client_6a733b40dbfc47c03217f3861d9619b0&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;nobody&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2017-10-10 01:17:29&lt;/sys_updated_on&gt;&lt;table/&gt;&lt;type&gt;onLoad&lt;/type&gt;&lt;ui_type&gt;10&lt;/ui_type&gt;&lt;variable_set display_value="SC_FUNCTIONS"&gt;0f63b740dbfc47c03217f3861d961971&lt;/variable_set&gt;&lt;view/&gt;&lt;/catalog_script_client&gt;&lt;/record_update&gt;</payload>
